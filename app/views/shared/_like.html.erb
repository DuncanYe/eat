<% if restaurant.is_liked?(current_user) %>
  <%= link_to 'Unlike', unlike_restaurant_path(restaurant), method: :post, data: { "disable-with": "Waiting...."}, class: "btn btn-dark" %>
<% else %>
  <%= link_to 'Like', like_restaurant_path(restaurant), method: :post, data: {"disable-with": "Waiting...."}, class: "btn btn-primary" %>
<% end %>